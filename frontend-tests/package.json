{
  "name": "frontend-tests",
  "version": "1.0.0",
  "description": "A suite of tests for the bevillingsplatform frontend. Tests are isolated from the frontend src. This way they can be run independently of the development server and corresponding docker container.",
  "scripts": {
    "test": "./node_modules/.bin/testcafe chrome tests/*.js --assertion-timeout 15000 --baseurl http://localhost:8080/",
    "headlesstest": "./node_modules/.bin/testcafe chrome:headless tests/*.js --assertion-timeout 15000 --selector-timeout 15000 --baseurl http://localhost:8080/"
  },
  "keywords": [],
  "author": "",
  "license": "MPL-2.0",
  "devDependencies": {
    "axe-core": "^3.5.3",
    "axe-testcafe": "^3.0.0",
    "resemblejs": "^3.2.4",
    "testcafe": "^1.8.5"
  },
  "repository": "https://git.magenta.dk/bevillingsplatform/bevillingsplatform.git"
}
